meta {
  name: Create Oauth Token
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/oauth/token
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{notionToken}}
}

body:json {
  {
    "grant_type": "authorization_code",
    "code": "abc123",
    "redirect_uri": "https://example.com/callback"
  }
}
